<head>
  <script src="/tracker-config.js"></script>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" type="text/css" href="/tracker-styles.css">
  <script>
    $(function() {
      $('#saveButton').click(function() {
        OneConfig.writeConfig({
          'symbol': $('#symbol').attr('value')
        });
        return false;
      });
    });
  </script>
</head>

<body>
  <form class="one-config">
    <fieldset>
      <label for="symbol">Symbol</label>
      <input id="symbol" name="symbol" placeholder="ex: TWTR">
    </fieldset>
    <input type="submit" id="saveButton" value="Save">
  </form>
</body>
